
% Limite

n= [1 10 100 500 1000 2000 4000 8000];
e = (1 + (1./n)^.n)

%Matriz diagonal
A= [2 6;3 9], B= [1 2;3 4], C= [-5 5;5 3];
G= blkdiag(A,B,C)

%%Fibonacci


%Función graficar
>> x=-10:0.001:-5;
>> y1=2+sin(x);
>> plot(x,y1)
>> hold on
>> x=-5:0.001:2;
>> y2=exp(x);
>> plot(x,y2)
>> hold on
>> x=2:0.001:10;
>> y3=log(x.^2+1);
>> plot(x,y3)

%%Ecuación lineal
r=10;
A=[5 2*r r; 3 6 ((2*r)-1); 2 (r-1) 3*r];
B=[2;3;5];
x= linsolve(A,B
